{
	"name": "obsidian-cycle-line",
	"version": "1.0.0",
	"description": "This plugin for Obsidian allows you to cycle through and change the line you are currently in.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"format": "prettier . --write",
		"lint": "eslint .",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "Christopher Bussick",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.34.0",
		"@types/node": "^16.11.6",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"tslib": "2.4.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0"
	}
}
